<body>
	<video width="500px" height="400px"
	       src = "mov_bbb.mp4" poster="mov_bbb.JPG"
	       onmouseover="over(this)" onmouseout="out(this)">
	</video>
	
	<script type="text/javascript">
		function over(vid){
			vid.play();
		}
		
		function out(vid){
			vid.pause();
			vid.load();
		}
	</script>
</body>
